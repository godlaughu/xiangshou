/**index.wxss**/
/* head */
.head {
    background-color: #ffffff;
    padding-left: 41rpx;
}
.search {
   display: flex;
   height: 43rpx;
   margin-bottom: 27rpx;
}
.icon-sousuo {
    font-size: 43rpx !important;
    color: #ff5597;
}
.img {
    width: 40rpx;
    height: 43rpx;
}
.inp {
    color: #b2b2b2;
    font-size: 16px;
    height: 43rpx;
    line-height: 40rpx;
    padding-left: 36rpx;
}

/* banner */
.banner {
    height: 250rpx;
    background-color: #e2e2e2;
}
swiper-item image {
    width: 100%;
    height: 250rpx;
}

/* home-food-search */
.home-food-search {
    height: 213rpx;
    background-color: #ffffff;
    display: flex;
    justify-content: space-around;
    margin-left: 20rpx;
    margin-right: 20rpx;
    padding-bottom: 18rpx;
}
.food-img {
    width: 123rpx;
    height: 123rpx;
    border-radius: 50%;
    margin: 22rpx 0 27rpx 0;
    display: flex;
}
.food-img .icon-icons-bread, .food-img .icon-shipu, .food-img .icon-ren, .food-img .icon-shijian {
    font-size: 63rpx;
    margin: auto;
    color: #ffffff;
}
.food {
    background: linear-gradient(#a681e6, #f879b7);
}
.recipe {
    background: linear-gradient(#50c9d9, #65a4e2);
}
.people {
    background: linear-gradient(#06e1b2, #42e74a);
}
.record {
    background: linear-gradient(#fde027, #ffbe1f);
}
.txt {
    font-size: 14px;
}

/* slimming */
.slimming {
    display: flex;
    background-color: #eaeaee;
}
.slimming-box {
    box-sizing: border-box;
    display: flex;
    justify-content: space-around;
    margin: auto;
    box-sizing: border-box;
}
.left {
    width: 345rpx;
    margin-right: 21rpx;
    background-color: #ffffff;
    position: relative;
}
.left, .community, .plan {
    border-radius: 8%;
}
.community {
    margin-bottom: 21rpx;
}
.community, .plan {
    background-color: #ffffff;
    height: 163rpx;
    position: relative;
}

.left, .right {
    margin-top: 21rpx;
    margin-bottom: 21rpx;
    height: 347rpx;
}
.right {
    width: 347rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
}
.community, .plan {
    height: 185rpx;
}
.course-img {
    position: absolute;
    bottom: 0;
    right: 0;
    padding-right: 21rpx;
}
.community-img, .plan-img {
    position: absolute;
    bottom: 0;
    right: 0;
    margin-right: 21rpx;
}
.course-img image {
    width: 246rpx;
    height: 139rpx;
}
.community-img image {
    width: 85rpx;
    height: 73rpx;
}
.plan-img image {
    width: 82rpx;
    height: 80rpx;
}
.left-box, .community-box, .plan-box {
    padding: 20rpx;
}
.course-title, .community-title, .plan-title {
    font-size: 16px;
    font-weight: 700;
    margin-bottom: 10rpx;
}
.course-txt, .community-txt, .plan-txt {
    font-size: 12px;
    color: #adadad;
}
.course-title {
    color: #aff2b5;
}
.community-title {
    color: #ffbe1f;
}
.plan-title {
    color: #f379b7;
}

/* comment */
.comment {
    background: #ffffff;
}
.comment-box {
    margin: 20rpx;
    border-bottom: 2px solid #cccccc;
}
.comment-title {
    font-size: 18px;
    margin-bottom: 20rpx;
}
.comment-bg {
    width: 100%;
    height: 357rpx;
    position: relative;
}

.comment-bg image {
    width: 100%;
    height: inherit;
}
.bg-cover {
    width: 100%;
    height: inherit;
    position: absolute;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.6);
}
.comment-txt {
    color: #ffffff;
    text-align: center;
}
.comment-txt-title {
    padding-top: 77rpx; 
    padding-bottom: 24rpx;
    font-size: 20px;
    font-weight: 600;
}
.comment-txt-title-txt {
    font-size: 14px;
}
.comment-txt-title, .comment-txt-title-txt {
    max-width: 380rpx;
    margin: auto;
}
.comment-bot {
    color: #b2b2b2;
    margin-top: 40rpx;
    padding-bottom: 20rpx;
    display: flex;
    font-size: 16px;
}
.comment-box2 {
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 2px solid #cccccc;
    margin: 23rpx;
}
.comment-bot-comments {
    margin-right: 38rpx;
}
.comment-left {
    width: 374rpx;
}
.comment-title-2 {
    font-size: 17px;
    margin-bottom: 60rpx;
}
.comment-right {
    margin-bottom: 18rpx;
}
.comment-right image {
    width: 238rpx;
    height: 160rpx;
}
.footer {
    text-align: center;
    margin: 23rpx;
    color: #363636;
    position: relative;
}
.online-consulting {
    position: absolute;
    bottom: 0;
    right: 0;
    background: #b2b2b2;
    width: 80rpx;
    height: 40rpx;
    font-size: 16rpx; 
    border-radius: 25%;
    line-height: 40rpx;
    color: #ffffff;
}
