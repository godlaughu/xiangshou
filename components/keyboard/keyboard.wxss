
.num-input{
   background: rgba(0, 0, 0, 0);
  position: absolute;
  top:-250rpx;
  left:250rpx;
}
.i-title{
  background: rgba(0, 0, 0, 0);
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  color: white;
  font-family: PingFangSC-Regular;
  font-size:36rpx; 
}
.onput{
  margin-top: 56rpx;
  background: rgba(0, 0, 0, 0);
  height: 87rpx;
  text-align: center;
  line-height: 72rpx;
  color: white;
  font-family: PingFangSC-Regular;
  font-size:72rpx; 
  display: flex;
  align-items: flex-end;
}
.underline{
  text-align: center;
  width: 250rpx;
  height: 72rpx;
  border-bottom: 1rpx solid;
}
.unit {
  font-size: 36rpx;
  line-height: 36rpx;
}
.keyboard {
    width: 750rpx;
    height: 669rpx;
    display: flex; 
    flex-flow:row wrap; 
}
.keyboard .ready {
   justify-content: space-between;
    background-color: rgb(236, 236, 236);
    flex-flow:row ; 
    width: 750rpx;
    height: 90rpx;
   display: flex;
} 
.keyboard .ready .cancel{
  color: rgb(178, 178, 178);
  margin-left: 15rpx;
  line-height: 90rpx;
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
}
.keyboard .ready .confirm{
  color: rgb(255, 84, 150);
  margin-right: 15rpx;
  line-height: 90rpx;
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
}
.keyboard .number {
    background-color: rgb(204, 204, 204);
    display: flex;
    width: 750rpx;
    height: 579rpx;
    flex-wrap: wrap;
}
 
.keyboard .number .num {
    width: 230rpx;
    height: 126rpx;
    font-size: 60rpx;
    text-align: center;
    font-weight: bold;
    margin-left:15rpx;
    margin-top: 15rpx; 
    background: white;
    border-radius : 20rpx;
   font-family: PingFangSC-Medium;
}
 .keyboard .number view .num1{
   font-family: PingFangSC-Medium;
   font-size: 60rpx;
   height: 60rpx;
   margin-top: 10rpx ;
 }
.keyboard .number view .num2{
  font-family: PingFangSC-Medium;
  font-size: 26rpx;
  height: 26rpx;
  margin-top: 10rpx ;
 }
 .keyboard .number .wu{
  background-color: rgb(204, 204, 204);
  line-height: 126rpx;
 }
 .keyboard .number .zero{
  line-height: 126rpx
 }
 .keyboard .number .delete{
   line-height: 126rpx
 }
.keyboard .number .active {
  background-color: rgb(236, 236, 236);
}
 



